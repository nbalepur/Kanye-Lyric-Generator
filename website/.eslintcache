begin\") {\n      return \"\";\n    }\n    if (compState === \"loading\") {\n      return <Loading theme={this.state.theme} />;\n    }\n    if (compState === \"lyrics\") {\n      return <Lyric theme={this.state.theme} lyric={this.state.lyrics} />;\n    }\n    if (compState === \"error\") {\n      return <Error theme={this.state.theme} />;\n    }\n  };\n\n  componentDidMount() {\n    document.body.style = \"background: \" + this.state.theme.mainBG;\n  }\n\n  render() {\n    return (\n      <div>\n        <Header\n          handleAPI={this.handleAPI}\n          numWords={this.state.numWords}\n          setNumWords={(val) => {\n            this.setState({ numWords: val });\n          }}\n          theme={this.state.theme}\n        />\n        <div>\n          <br></br>\n          <br></br>\n          {this.getComponentFromState()}\n          <br></br>\n          <br></br>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default App;\n","C:\\Users\\nishu\\Kanye-Song-Generator\\website\\src\\reportWebVitals.js",[],"C:\\Users\\nishu\\Kanye-Song-Generator\\website\\src\\components\\header.js",[],"C:\\Users\\nishu\\Kanye-Song-Generator\\website\\src\\components\\loading.js",[],"C:\\Users\\nishu\\Kanye-Song-Generator\\website\\src\\components\\lyric.js",[],"C:\\Users\\nishu\\Kanye-Song-Generator\\website\\src\\components\\error.js",[],"C:\\Users\\nishu\\Kanye-Song-Generator\\website\\src\\themes.js",[],{"ruleId":"39","replacedBy":"40"},{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","severity":1,"message":"44","line":12,"column":16,"nodeType":"45","messageId":"46","endLine":12,"endColumn":21},"no-native-reassign",["47"],"no-negated-in-lhs",["48"],"no-unused-vars","'light' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]